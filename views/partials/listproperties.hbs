{{#each properties}}
  <section class="ui stacked segment">
    <h2 class="ui header">
      {{address}}
    </h2>
    <a href="/property/{{id}}" class="ui icon button">
      <i class="icon folder open"></i>
    </a>
    <a href="/dashboard/deleteproperty/{{id}}" class="ui red icon button">
      <i class="icon trash"></i>
    </a>
   </section>

{{/each}}

